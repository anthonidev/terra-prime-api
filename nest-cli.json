{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "assets": [
      "src/chatbot/context/*.json",
      {
        "include": "reports/reports-leads/img/*.png",
        "outDir": "dist"
      }
    ],
    "watchAssets": true,
    "deleteOutDir": true
  }
}